def deleteUpperCase(s):
    r = ""
    for letter in s:
        if letter not in 'ABCDEFGHIJKLMNOPQRSTUVWXYZ':
            r = r + letter
    return r
print deleteUpperCase("HEeO")


def deleteUpperCase(s):
    r = ""
    for letter in s:
        if letter not in 'ABCDEFGHIJKLMNOPQRSTUVWXYZ':
            r = r + letter
    print "'"+ r +"'"
deleteUpperCase("HheO")
