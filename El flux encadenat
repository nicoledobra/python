def function1(x, s = "hello"):
    for letter in s:                               
        print letter * x                                    
    return len(s)                                    
x = "program"                                           
y = function1(2, x)                                         
if y > 4:                                         
    print "Total length is: %d" % (function1(2))
else:            
    print "End of program"

