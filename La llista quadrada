def squareNumbers(n):
    squares = []
    for i in range(1, n+1):
        squares.append(i**2)
    return squares
print squareNumbers(10)

R: [1, 4, 9, 16, 25, 36, 49, 64, 81, 100]

def squareNumbers(n):
    squares = [i**2 for i in range(1, n+1)]
    return squares
print squareNumbers(-1)


def squareNumbers(n):
    numbers = range(1, n+1)
    squares = map(lambda x: x**2, numbers)
    return list(squares)
print squareNumbers(4)
